{
  "name": "frontend",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "npx eslint \"src/**/*.{ts,js,vue}\"",
    "lint:fix": "npx eslint \"src/**/*.{ts,js,vue}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,js,vue}\" --ignore-unknown",
    "format:check": "prettier --check \"src/**/*.{ts,js,vue}\" --ignore-unknown",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint",
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@primeuix/themes": "^1.0.0",
    "@primevue/themes": "^4.5.2",
    "axios": "^1.13.2",
    "chart.js": "3.3.2",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.1",
    "tailwindcss-primeui": "^0.6.0",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@primevue/auto-import-resolver": "^4.3.1",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tailwindcss/vite": "^4.1.17",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^9.23.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.2.5",
    "sass": "^1.55.0",
    "semantic-release-preconfigured-conventional-commits": "^1.1.152",
    "tailwindcss": "^4.1.17",
    "unplugin-vue-components": "^0.27.3",
    "vite": "^7.2.4"
  }
}
