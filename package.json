{
  "name": "frontend",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint --fix . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write \"src/**/*.{ts,js,vue}\" --ignore-unknown",
    "format:check": "prettier --check \"src/**/*.{ts,js,vue}\" --ignore-unknown",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint",
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^9.23.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.2.5",
    "semantic-release-preconfigured-conventional-commits": "^1.1.152",
    "vite": "^7.2.4"
  }
}
